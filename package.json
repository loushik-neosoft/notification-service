{
  "name": "notification-service",
  "version": "1.0.0",
  "description": "Email Notification Service",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "build": "tsc && tsc-alias",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "migrate": "knex migrate:latest --knexfile src/database/knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile src/database/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile src/database/knexfile.ts",
    "codegen": "kysely-codegen --out-file src/database/types.ts --dialect postgres"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bull": "^4.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.0.0",
    "knex": "^3.1.0",
    "kysely": "^0.28.10",
    "nodemailer": "^7.0.12",
    "pg": "^8.11.3",
    "prom-client": "^15.0.0",
    "rate-limit-redis": "^4.0.0",
    "redis": "^4.6.10",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@types/nodemailer": "^7.0.5",
    "@types/pg": "^8.10.7",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.15",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "kysely-codegen": "^0.19.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}